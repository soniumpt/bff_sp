[function staff]
IF	( <src.tag.staff_title> ==0 )
	SYSMESSAGE=@07a1 Staff title turned On
	src.tag.staff_title=1
	events +e_zoragath_staff_title
ELSE
	SYSMESSAGE=@07a1 Staff title turned Off
	src.tag.staff_title=0
	events -e_zoragath_staff_title
ENDIF

