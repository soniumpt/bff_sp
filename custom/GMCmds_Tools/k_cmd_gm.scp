endfor
RETURN 1


//********************************************************************//
//                            CLEARCONTITEMS                          //
//     DELETE X FROM PACKS <ARGN> TILES AWAY FROM FUNCTION CALLER     //
//********************************************************************//
[FUNCTION clearcontitems]
obj=<uid>
foritems 10000
   if (<type>=t_container)
      for <fcount>
         forcont <uid> 1
            if (<baseid>==i_dagger_copper) 
                          
                  src.bounce <UID>
                  serv.log removing <UID>
                  REMOVE
              
            endif
         endfor
      endfor
   endif
endfor
RETURN 1

