[FUNCTION despawn_world]
local.i=0
forchars 9999
   ref1 = <MemoryFindType.memory_ispawned.link>
   if (<ref1>)
      if (<ref1.type> == t_spawn_char)
		 ref1.remove
         remove
         local.i += 1
      endif
   endif
endfor
sysmessage @00a24 Total spawns removed: <dlocal.i>


[FUNCTION respawnnpcs]
local.i=0
FORCHARS 9000
	LOCAL.AMISPAWNED	= <MEMORYFINDTYPE.memory_ispawned.LINK>
	IF (<LOCAL.AMISPAWNED> && (<FLAGS> ~statf_invul) )
		UID.<LOCAL.AMISPAWNED>.TIMER = (<UID.<LOCAL.AMISPAWNED>.MOREX> * 60)		
        REMOVE
		 local.i += 1
	ENDIF
ENDFOR
serv.announce Total spawns respawned: <dlocal.i>


[FUNCTION wipeaccounts]
local.i=0
FOR 845
	IF (STRCMP(<SERV.ACCOUNT.<local.i>.NAME>, ray_staff) == 0)
		local.i += 1
	ELSEIF (STRCMP(<SERV.ACCOUNT.<local.i>.NAME>, arkos_staff) == 0)
		local.i += 1
	ELSEIF (STRCMP (<SERV.ACCOUNT.<local.i>.NAME>, pink_staff) == 0)
		local.i += 1
	ELSEIF (STRCMP (<SERV.ACCOUNT.<local.i>.NAME>, starbuck_staff) == 0)
		local.i += 1
	ELSE
		serv.account <SERV.ACCOUNT.<local.i>.NAME> delete
	ENDIF
ENDFOR



