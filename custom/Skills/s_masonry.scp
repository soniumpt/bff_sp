//********************************************************************//
[FUNCTION f_mason_make]
IF (<SRC.MASONRY> >= 850)
   MENU m_mason
ELSEIF (<SRC.MASONRY> >= 600)
   MENU m_mason_2
ELSEIF (<SRC.MASONRY> >= 500)
   MENU m_mason_3
ELSEIF (<SRC.MASONRY> >= 250)
   MENU m_mason_4
ELSEIF (<SRC.MASONRY> >= 0)
   SRC.SYSMESSAGE You lack sufficient Masonry skill to use this item
   RETURN 1
ENDIF

