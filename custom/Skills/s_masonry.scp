//********************************************************************//
//                              FUNCTIONS                             //
//********************************************************************//
[FUNCTION MASON_CANMAKE]
   IF !<SRC.CANMAKE <args>>
      SRC.SYSMESSAGE You cannot make that item.
      RETURN 0
   ELSE
      f_mason_delay
      IF (<SRC.TAG.MASON.DELAY> = 1)
         RETURN 0
      ENDIF
   ENDIF
   RETURN 1

//********************************************************************//
[FUNCTION f_mason_makeitem]
   SRC.NEWITEM = i_mason_makeitem
   SRC.ACT.CONT=<SRC.UID>
   SRC.ACT.TIMER=1

//********************************************************************//
[ITEMDEF i_mason_makeitem]
ID=i_memory
TYPE=t_eq_script
NAME=Masonry Item Maker

ON=@CREATE
   MORE=0 
   ATTR=attr_decay

ON=@TIMER
   IF (<MORE>=<EVAL(<CONT.TAG.MASON.MAKETIME>)>) //MAKES DELAY LOOP ONCE BEFORE DECAYING/MAKING ITEM
      CONT.NEWITEM <CONT.TAG.MASONITEM>
      CONT.ACT.CONT = <CONT.UID>
      CONT.TAG.MASONITEM
      SOUND 023d
      CONT.f_mason_gain
      REMOVE
   ELSE
      TIMER=1
      SOUND 023d
      MORE= (<MORE> + 1) //CHANGES MORE VALUE SO ITEM DECAYS NEXT TIMER LOOP
   ENDIF
   RETURN 1

//********************************************************************//
[FUNCTION f_mason_delay]
   IF (<SRC.FINDID.i_mason_gain_delay.UID>)
      SRC.MESSAGE "You must wait to use this skill again."
      SRC.TAG.MASON.DELAY = 1
   ELSE
      SRC.TAG.MASON.DELAY = 0
      SRC.NEWITEM = i_mason_gain_delay
      SRC.ACT.CONT=<SRC.UID>
      SRC.ACT.TIMER=1
   ENDIF

//********************************************************************//
[ITEMDEF i_mason_gain_delay]
ID=i_memory
TYPE=t_eq_script
NAME=Masonry Gain Delay

ON=@CREATE
   MORE=0
   ATTR=attr_decay

ON=@TIMER
   IF (<MORE>)
      REMOVE
   ELSE
      TIMER=<CONT.TAG.MASON.MAKETIME>
      MORE=1
   ENDIF
   RETURN 1

