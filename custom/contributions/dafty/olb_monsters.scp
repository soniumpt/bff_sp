[SPEECH spk_llama_mastertimer1]

on=*start*

IF !(<SRC.ACCOUNT.PLEVEL> >= 2)
SAY You cant do that
RETURN 1
ENDIF

If (<tag.gatesopen>==01)
Say Gates are already open
Else
MESSAGEua 00085,0,0,eng, TIMER ACTIVATED
serv.newitem m_llamaracetimer_delayA
EQUIP <NEW.UID>
serv.newitem m_llamaracetimer_delay2A
EQUIP <NEW.UID>
Return 1
ENDIF


on=*close*

IF !(<SRC.ACCOUNT.PLEVEL> >= 2)
SAY You cant do that
return 1
endif

if (<tag.gatesopen>==00)
Say Gates are already closed
else
Say Gates closed
ACT = <UID.0438d1f28> 
act.nudgeup 20
ACT = <UID.0438d1f27> 
act.nudgeup 20
ACT = <UID.0438d1f31> 
act.nudgeup 20
ACT = <UID.0438d1f29> 
act.nudgeup 20
ACT = <UID.0438d1f2b> 
act.nudgeup 20


tag.gatesopen =
tag.gatesclosed = 1
return 1

else

SAY Gates are already closed
return 1
endif


[ItemDef M_llamaracetimer_DelayA]
id=i_memory
Name=Llama Race Timer DelayA
type=t_eq_script

ON=@Create
TIMER = 1
MORE1 = 6

ON=@Timer
If (<more1> == 6)
cont.sayua 00085,0,0,eng, 5
timer = 1
more1 = 5
elseif (<more1> == 5)
cont.sayua 00085,0,0,eng, 4
timer = 1
more1 = 4
elseif (<more1> == 4)
cont.sayua 00085,0,0,eng, 3
timer = 1
more1 = 3
elseif (<more1> == 3)
cont.sayua 00085,0,0,eng, 2
timer = 1
more1 = 2
elseif (<more1> == 2)
cont.sayua 00085,0,0,eng, 1
timer = 1
more1 = 1
elseif (<more1> == 1)
cont.sayua 00085,0,0,eng, START THE RACE!!!!!!!!
remove
endif
return 1

ON=@UNEQUIP

CONT.TAG.gatesopen=1
CONT.tag.gatesclosed=

OBJ = <UID.0438d1f28> 
OBJ.nudgedown 20
OBJ = <UID.0438d1f27> 
OBJ.nudgedown 20
OBJ = <UID.0438d1f31> 
OBJ.nudgedown 20
OBJ = <UID.0438d1f29> 
OBJ.nudgedown 20
OBJ = <UID.0438d1f2b> 
OBJ.nudgedown 20


