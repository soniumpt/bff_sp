// Esta function nao é preciso mexer, basta haver so uma
 
[FUNCTION f_rare_chest_restock125a]
IF (<EVAL <MORE1>>==0)
        IF (<COUNT> > 0)
                FORCONT <UID> 0
                        REMOVE
                ENDFOR
        ENDIF
        TAG.UID=
        MORE1=1
        TIMER=<DEF.rare_chests_restock>
ELSE
        SERV.NEWITEM <ARGS>
        NEW.CONT=<UID>
        LOCAL.UID1=<UID>
        FORCONT <LOCAL.UID1> 0
                IF (<BASEID>==i_bag)
                        LOCAL.UID2=<UID>
                        FORCONT <LOCAL.UID2> 0
                                CONT=<LOCAL.UID1>
                        ENDFOR
                        REMOVE
                ENDIF                  
        ENDFOR
        MORE1=0
        TIMER=-1
ENDIF





