IF !(<SRC.ISGM>)
        IF (<EVAL <MORE1>>==0)
                IF !(STRMATCH("<TAG.UID>",""))
                        IF !(<TAG0.UID>==<SRC.UID>)
                                SRC.SYSMESSAGE This Imperial Chest has already been open by another Player.
                                RETURN 1
                        ENDIF
                ELSE
                        IF (<SRC.RESTEST 1 i_imperial_SHAMBLING_key>) // Mudar ID da chave
                                SRC.SYSMESSAGE You've opened the Imperial Shambling Chest. You will be able to use it for the next <EVAL (<DEF.rare_chests_timer>/60)> minutes. // Mudar a mensagem.
                                SRC.CONSUME 1 i_imperial_SHAMBLING_key // Mudar ID da chave
                                TAG.UID=<SRC.UID>
                                TIMER=<DEF.rare_chests_timer>
                        ELSE
                                SRC.SYSMESSAGE You need a Imperial Shambling Chest Key in order to open this Imperial Chest. // Mudar a mensagem pa outro chest
                                RETURN 1
                        ENDIF
                ENDIF
        ELSE
                SRC.SYSMESSAGE You don't think there is anything worth inside of this chest.
                RETURN 1
        ENDIF
ENDIF
 
