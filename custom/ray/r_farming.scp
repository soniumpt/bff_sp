[itemdef i_farming_water]
id=i_tub_water
name = farming tub of water
value = 100

on @targon_item
if (<src.targ.baseid> = i_farming_soil_f)
	src.targ.id=i_farming_soil_fw
	src.targ.color = 036
	src.emote Water the soil
	id=i_farming_tub_empty
	update
	local.rand <Eval rand(1,45)>
	if (<local.rand>==1)
		if (<SRC.FINDID.i_afk_Check_countdown>==0)
			serv.log serv.log  AFK Checking System, <src.name> (<src.account>) is probably AFK Macroing FARMING
			src.tag.afkskill = SKILL_FARMING
			src.tag.probablyafk = 1
			src.tag.spamdialog = 1
			src.newitem i_afk_check_countdown
			src.act.equip
			src.tag.counter
			SRC.DIALOG d_afk_check
		endif
	endif
else
	return 1
endif
return 1

ON @DCLICK
target What do you want to Water
return 1

on=@dropon_self
return 1


