TIMER 10
ELSEIF (<MORE1>==4)
TIMER 1
ENDIF

ON=@TIMER
CONT.NEWITEM i_drug_effect
IF (<MORE1>==1)
CONT.ACT.MORE1 1
ELSEIF (<MORE1>==2)
CONT.ACT.MORE1 2
ELSEIF (<MORE1>==3)
CONT.ACT.MORE1 3
ELSEIF (<MORE1>==4)
CONT.ACT.MORE1 4
ENDIF
CONT.ACT.CONT=<CONT>
CONT.SYSMESSAGE The drug has taken effect!
REMOVE

RETURN 1

