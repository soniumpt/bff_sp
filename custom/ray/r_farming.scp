if (<src.targ.baseid> = i_farming_soil)||(<src.targ.baseid>=031fb)
	src.targ.id=i_farming_soil_f
	src.targ.color = 033
	src.emote Fertilize the soil
	local.rand <Eval rand(1,45)>
	if (<local.rand>==1)
		if (<SRC.FINDID.i_afk_Check_countdown>==0)
			serv.log serv.log  AFK Checking System, <src.name> (<src.account>) is probably AFK Macroing FARMING
			src.tag.afkskill = SKILL_FARMING
			src.tag.probablyafk = 1
			src.tag.spamdialog = 1
			src.newitem i_afk_check_countdown
			src.act.equip
			src.tag.counter
			SRC.DIALOG d_afk_check
		endif
	endif	
	if (<amount> > 1)
		amount = <eval <amount> - 1>
	else
		remove
	endif
endif

return 1

ON @DCLICK
color 01bb
target What do you want to feritlize
return 1

