[itemdef i_farming_seed_fruit_watermelon]
id=i_gold
type = t_normal
name = Water Melon seed
resources=i_farming_seed
skillmake=FARMING 50.0
ON @CREATE
color = 039
on @targon_item
if (<src.farming> > 499)
	if (<src.targ.baseid> = i_farming_soil_t)
		if (<SRC.DISTANCE <SRC.TARG>> <= 1)
			if (<cont.id> = <src.findlayer(21).id>)
				src.emote plantplant <name>
				src.newitem i_farming_plant_1
				src.act.tag.plant = i_fruit_watermelon
				src.act.tag.amount = <eval rand(5,10)>
				src.act.p = <src.targ.p>
				src.act.tag.skill = 500
				src.act.tag.planter = <src.uid>
				src.act.tag.soil = <src.targ.uid>
				src.targ.id=i_farming_soil_p
				src.update
				src.consume <baseid>
				return 1
			else
				src.sysmessage The seed must be in your pack
				return 1
			endif
		else
			src.sysmessage You are too far from the soil.
			return 1
		endif
	else
		src.sysmessage You must selected tilled soil.
		return 1
	endif
else
	src.sysmessage You do not have enough farming to plant this.
	return 1
endif
ON @DCLICK
target Where do you want to plant this?
return 1
