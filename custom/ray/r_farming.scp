ON=@CREATE
ATTR=attr_decay
TAG.TIMER=<TIMER>
TIMER=1
MORE2=1

ON=@TIMER
IF (<MORE2>==1)
CONT.TAG.OLDFLAG=<CONT.FLAGS>
CONT.FLAGS=<CONT.FLAGS>|0400000
CONT.EVENTS +e_drugs
CONT.UPDATE
IF (<MORE1>==1)
TIMER=60
CONT.STR=105
CONT.DEX=95
CONT.INT=90
ELSEIF (<MORE1>==2)
TIMER=120
CONT.STR=110
CONT.DEX=90
CONT.INT=85
ELSEIF (<MORE1>==3)
TIMER=180
CONT.STR=115
CONT.DEX=85
CONT.INT=70
ELSEIF (<MORE1>==4)
TIMER=240
CONT.STR=120
CONT.DEX=80
CONT.INT=75
ENDIF
MORE2=2
ELSE
CONT.EVENTS -e_drugs
CONT.FLAGS=<CONT.TAG.OLDFLAG>
CONT.UPDATE
CONT.STR=100
