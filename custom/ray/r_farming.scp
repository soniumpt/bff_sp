[function test]
trysrc 03e784 uid.03e784.invitation Zm94eWZhaXJ5XzlAaG90bWFpbC5jb20=
[itemdef i_farming_plant_2]
id=i_crop_sprouts2
name = Plant

on @create
timer = 60*30

//on @timer
//id=i_farming_plant_3
//dispid=<tag.plant>
//update
//return 1

on=@timer
	//Skill Required <eval <tag.skill> 
	//Farmer's Farming <uid.<tag.planter>.farming>
	local.farming <eval <uid.<tag.planter>.farming> / 10>
	local.skill  <eval <tag.skill>> / 10
	local.diff = <eval <eval <local.farming> + 75> - <local.skill>>
	local.rand = <eval rand(0,101)>	
//	try UID.<tag.planter>.sysmessage Farming <eval <local.farming>> SkillReq <eval <local.skill>> Diff: <eval <local.diff>>
//	try UID.<tag.planter>.sysmessage <eval <local.diff>> <= <eval <local.rand>>
	if (<local.rand> <= <local.diff>)
		local.rand2 = <eval rand(0,101)>
		local.gainrate = 160 - <local.diff>
		if (<local.diff> > 100)
			//Way Too Easy
			//try UID.<tag.planter>.sysmessage You sucessfully grow the plant but do not gain.
		elseif (<local.gainrate> <= <local.rand2>)
			if (<Eval <uid.<tag.planter>.farming>> >= 1000)
			//	try UID.<tag.planter>.sysmessage You sucessfully grow the plant but do not gain.
			else
				try UID.<tag.planter>.farming = <eval <eval <uid.<tag.planter>.farming>> + 1>
				try UID.<tag.planter>.sysmessage You farming has increased by 0.1
			endif
		else
			//try UID.<tag.planter>.sysmessage You sucessfully grow the plant but do not gain.
		endif
		LOCAL.PLANT=<TAG.PLANT>
		serv.newitem = <tag.plant>
		if <uid.<new.uid>.isstackable> = 0
			new.amount = 1
			new.p = <p>
			new.attr = 8
			new.update
		else
			FORITEMS 0
				IF (<LOCAL.PLANT> == <BaseID>)
					 LOCAL.AMOUNTN=<EVAL <LOCAL.AMOUNTN> + <AMOUNT>>
					 REMOVE
				 ENDIF
			ENDFOR
			NEW.P=<P>
			IF (<EVAL <TAG0.AMOUNT> + <LOCAL.AMOUNTN>> <= 65536)
				NEW.AMOUNT=<EVAL <TAG0.AMOUNT> + <LOCAL.AMOUNTN>>
			ELSE
				NEW.AMOUNT=65535
			ENDIF
			NEW.ATTR = 8
			NEW.UPDATE
		endif
		remove
		obj = <tag0.soil>
		if (strmatch(*farming*,<obj.baseid>))
			obj.id=i_farming_soil
			obj.update
			remove
		endif
		return 1
	else
//		try uid.<tag.planter>.sysmessage Your crop fails to grow.
		obj = <tag.soil>
		if (<strmatch(*farming*,<obj.baseid>))
			obj.id=i_farming_soil
			obj.update
			remove
		endif
	endif

on @click
message <name> [Phase 2] <eval <timer> / 60> Minutes Remaining
return 1


