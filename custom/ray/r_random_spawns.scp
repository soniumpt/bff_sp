//*********************************************************************************************************//
//  Items to spawn the stones, so steeds don't spawn all at the same time if they spawn timer is the same  // 
//                                                                                                         //
//  Set the timer to 1 on the spawn replacer to start spawning the stones.                                 //
//																										   //
// The following can be spawned by hand:																   //
//																										   //	
// i_rareSpawn_l42 Unicorn/Shadow/Wraped Mustang                                                           //
// i_rareSpawn_l43 Nightmare                                                                               //
// i_rareSpawn_l45 Terathane Zos                                                                           // 
// i_rareSpawn_l46 Phoenix                                                                                 //  
//                                                                                                         //
//*********************************************************************************************************//

[ITEMDEF i_spawn_replacer_mustang]
ID=i_fx_yellow_dot
name = Spawn Replacer Mustang

ON=@CREATE
ATTR=08090
MORE1=1
TIMER=5

ON=@TIMER

IF (<MORE1>=1)
SERV.NEWITEM i_rarespawn_l1
NEW.P=<P>
MORE1=2
TIMER=3600
MOVE N
ELSEIF (<MORE1>=2)
SERV.NEWITEM i_rarespawn_l2
NEW.P=<P>
MORE1=3
TIMER=3600
MOVE N
ELSEIF (<MORE1>=3)
SERV.NEWITEM i_rarespawn_l3
NEW.P=<P>
MORE1=4
TIMER=3600
MOVE N
ELSEIF (<MORE1>=4)
SERV.NEWITEM i_rarespawn_l4
NEW.P=<P>
MORE1=5
TIMER=3600
MOVE N
ELSEIF (<MORE1>=5)
SERV.NEWITEM i_rarespawn_l5
NEW.P=<P>
MORE1=6
TIMER=3600
MOVE N
ELSEIF (<MORE1>=6)
SERV.NEWITEM i_rarespawn_l6
NEW.P=<P>
MORE1=7
TIMER=3600
MOVE N
ELSEIF (<MORE1>=7)
SERV.NEWITEM i_rarespawn_l7
NEW.P=<P>
MORE1=8
TIMER=3600
MOVE N
ELSEIF (<MORE1>=8)
SERV.NEWITEM i_rarespawn_l8
NEW.P=<P>
MORE1=9
TIMER=3600
MOVE N
ELSEIF (<MORE1>=9)
SERV.NEWITEM i_rarespawn_l10
NEW.P=<P>
MORE1=10
TIMER=3600
MOVE N
ELSEIF (<MORE1>=10)
SERV.NEWITEM i_rarespawn_l11
NEW.P=<P>
REMOVE
ENDIF
RETURN 1


