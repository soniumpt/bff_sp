//STARTS MUSTANGS 1-11


[ITEMDEF i_rareSpawn_l1]
ID=i_gravestone
TYPE=t_eq_script
Name=Random Spawn 1 Manager [Mustang]

ON=@CREATE
ATTR=08090
TIMERF 5, f_rarespawn_l1

[FUNCTION f_rarespawn_l1]

tag.lastspawn=<SERV.RTIME>

IF (0<var.rareSpawn.l1>)
TRY uid.<var.rareSpawn.l1>.remove
ENDIF

DORAND 2
tag.pos = 810,1922
tag.pos = 1003,361
ENDDO

DORAND 10
tag.type = mustang_grey
tag.type = mustang_roan
tag.type = mustang_chocolate
tag.type = mustang_redroan
tag.type = mustang_sky
tag.type = mustang_pamamino
tag.type = mustang_wimmimate
tag.type = mustang_skygray
tag.type = mustang_crimson
tag.type = mustang_black
ENDDO

NEWITEM i_worldgem_bit
var.rareSpawn.l1 = <new.uid>
NEW.P = <tag.pos>
NEW.TYPE 34
NEW.AMOUNT 1
NEW.MORE c_a_<tag.type>
NEW.MOREP 1440 1441 15
NEW.ATTR 00b0
NEW.TIMER 1
TIMERF 43200, f_rarespawn_l1
RETURN 1
    
