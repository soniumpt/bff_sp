// Name Event Disabled

[EVENTS e_brotherhood_name]

ON=@CLICK
IF ((<MEMORYFINDTYPE.memory_guild.UID>) && !(<FINDID.i_rune_incognito.uid>))
      OBJ=<MEMORYFINDTYPE.MEMORY_GUILD.LINK.UID>
      FOR 0 <EVAL <OBJ.MEMBER.COUNT>-1>
         IF (<OBJ.MEMBER.<EVAL <LOCAL._FOR>>.UID> == <UID>)
            TRY OBJ.MEMBER.<EVAL <LOCAL._FOR>>.SHOWABBREV=0
         ENDIF
      ENDFOR
      IF (0<TAG.GUILDABBREVIATION>)
         TRY MESSAGE @<f_guildabbreviation_color> [<UID.<memoryfindtype.memory_guild.LINK>.ABBREV>]
      ENDIF
   ENDIF
   
IF (<TAG0.BROTHERHOOD.SHOWTITLE> == 0) && !(<ISMURDERER>) && !(<ISKARMAEVIL>) && !(<ISKARMANEUTRAL>) && !(<FLAGS> & statf_criminal)
MESSAGE = @0770 <NAME> [B]
ELSEIF (<TAG0.BROTHERHOOD.SHOWTITLE> == 0) && (<ISMURDERER>) || (<ISKARMAEVIL>)
MESSAGE = @021 <NAME> [B]
ELSEIF (<TAG0.BROTHERHOOD.SHOWTITLE> == 0) && (<ISKARMANEUTRAL>) && (<FLAGS> & statf_criminal)
MESSAGE = <NAME> [B] [criminal]
ELSEIF (<TAG0.BROTHERHOOD.SHOWTITLE> == 0) && (<ISKARMANEUTRAL>)
MESSAGE = <NAME> [B]
ELSEIF (<TAG0.BROTHERHOOD.SHOWTITLE> == 0) && (<FLAGS> & statf_criminal)
MESSAGE = <NAME> [B] [criminal]
ELSEIF (<TAG0.BROTHERHOOD.SHOWTITLE> == 0) && (<FLAGS> & statf_criminal) && (<ISMURDERER>) || (<ISKARMAEVIL>)
MESSAGE = @021 <NAME> [B]
ENDIF

IF (<TAG0.BROTHERHOOD.SHOWTITLE> == 1) && !(<ISMURDERER>) && !(<ISKARMAEVIL>) && !(<ISKARMANEUTRAL>) && !(<FLAGS> & statf_criminal)
MESSAGE = @0770 <NAME>, <TAG.CURRENTBRANK>
ELSEIF (<TAG0.BROTHERHOOD.SHOWTITLE> == 1) && (<ISMURDERER>) || (<ISKARMAEVIL>)
MESSAGE = @021 <NAME>, <TAG.CURRENTBRANK>
ELSEIF (<TAG0.BROTHERHOOD.SHOWTITLE> == 1) && (<ISKARMANEUTRAL>) && (<FLAGS> & statf_criminal)
MESSAGE = <NAME>, <TAG.CURRENTBRANK> [criminal]
ELSEIF (<TAG0.BROTHERHOOD.SHOWTITLE> == 1) && (<ISKARMANEUTRAL>)
MESSAGE = <NAME>, <TAG.CURRENTBRANK>
ELSEIF (<TAG0.BROTHERHOOD.SHOWTITLE> == 1) && (<FLAGS> & statf_criminal)
MESSAGE = <NAME>, <TAG.CURRENTBRANK> [criminal]
ELSEIF (<TAG0.BROTHERHOOD.SHOWTITLE> == 1) && (<FLAGS> & statf_criminal) && (<ISMURDERER>) || (<ISKARMAEVIL>)
MESSAGE = @021 <NAME>, <TAG.CURRENTBRANK>
ENDIF
RETURN 1

