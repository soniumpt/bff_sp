elseif (<eval <src.targ.tag.totalrating>> > 6999 )&&(<eval <src.targ.tag.totalrating>> < 7999 )
src.targ.tag.skilltitle=Warlord
elseif (<eval <src.targ.tag.totalrating>> > 7999 )&&(<eval <src.targ.tag.totalrating>> < 8999 )
