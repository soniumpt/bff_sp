[FUNCTION ISBROTHERHOOD]
IF STRMATCH( "<TAG0.ALIGNMENT>", "alignment_brotherhood" )
RETURN 1
ENDIF
RETURN 0

[FUNCTION ISMERCENARY]
IF STRMATCH( "<TAG0.ALIGNMENT>", "alignment_mercenary" )
RETURN 1
ENDIF
RETURN 0

[FUNCTION ISSYNDICATE]
IF STRMATCH( "<TAG0.ALIGNMENT>", "alignment_syndicate" )
RETURN 1
ENDIF
RETURN 0

[FUNCTION ISACADEMY]
IF ((<TAG0.ACADEMY> == 01) || (<TAG0.ACADEMY> == 02))
RETURN 1
ENDIF
RETURN 0

[FUNCTION ISMILITANT]
IF ((<TAG0.MILITANT> == 01) || (<TAG0.MILITANT> == 02))
RETURN 1
ENDIF
RETURN 0

