   ELSEIF ( (<ACCOUNT.TAG0.ACHIEVEMENT.EVENT.LLAMARACE> >= 1) && !(<ACCOUNT.TAG0.ACHIEVEMENTS.SET4> & ach_won5llamaevents) )
