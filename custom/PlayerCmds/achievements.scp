[FUNCTION f_achievement_event_llamarace]
IF !(<ACCOUNT.TAG0.ACHIEVEMENTS.SET4>)
   ACCOUNT.TAG0.ACHIEVEMENTS.SET4=
ENDIF
IF !(<ACCOUNT.TAG0.ACHIEVEMENT.EVENT.LLAMARACE>)
   ACCOUNT.TAG0.ACHIEVEMENT.EVENT.LLAMARACE=1
   ACCOUNT.TAG0.ACHIEVEMENTS.SET1=<EVAL (<ACCOUNT.TAG0.ACHIEVEMENTS.SET4> | ach_wonfirstllamaevent)>
   SYSMESSAGE @07a1 [ACHIEVEMENT] Won First Llama Race Event
   SERV.LOG <NAME>:<UID> unlocked achievement: Won First Llama Race Event
   SYSMESSAGE
ELSE
   ACCOUNT.TAG0.ACHIEVEMENT.EVENT.LLAMARACE=<EVAL <ACCOUNT.TAG0.ACHIEVEMENT.EVENT.LLAMARACE> + 1> 
   IF ( (<ACCOUNT.TAG0.ACHIEVEMENT.EVENT.LLAMARACE> >= 1) && !(<ACCOUNT.TAG0.ACHIEVEMENTS.SET4> & ach_wonfirstllamaevent) )
      ACCOUNT.TAG0.ACHIEVEMENTS.SET1=<EVAL (<ACCOUNT.TAG0.ACHIEVEMENTS.SET1> | ach_killedfirstplayer)>   
      SYSMESSAGE @07a1 [ACHIEVEMENT] Won First Llama Race Event!
