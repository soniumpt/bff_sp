[FUNCTION f_achievement_kills]
IF !(<ACCOUNT.TAG0.ACHIEVEMENTS.SET1>)
   ACCOUNT.TAG0.ACHIEVEMENTS.SET1=
ENDIF
IF !(<ACCOUNT.TAG0.ACHIEVEMENT.KILLS>)
   ACCOUNT.TAG0.ACHIEVEMENT.KILLS=1
   ACCOUNT.TAG0.ACHIEVEMENTS.SET1=<EVAL (<ACCOUNT.TAG0.ACHIEVEMENTS.SET1> | ach_killedfirstplayer)>
   SYSMESSAGE @07a1 [ACHIEVEMENT] Killed first player
   SERV.LOG <NAME>:<UID> unlocked achievement: Killed first player
   SYSMESSAGE
ELSE
   ACCOUNT.TAG0.ACHIEVEMENT.KILLS=<EVAL <ACCOUNT.TAG0.ACHIEVEMENT.KILLS> + 1> 
   IF ( (<ACCOUNT.TAG0.ACHIEVEMENT.KILLS> >= 1) && !(<ACCOUNT.TAG0.ACHIEVEMENTS.SET1> & ach_killedfirstplayer) )
      ACCOUNT.TAG0.ACHIEVEMENTS.SET1=<EVAL (<ACCOUNT.TAG0.ACHIEVEMENTS.SET1> | ach_killedfirstplayer)>   
      SYSMESSAGE @07a1 [ACHIEVEMENT] Killed first player!
      SERV.LOG <NAME>:<UID> unlocked achievement: Killed first player
   ENDIF
   IF ( (<ACCOUNT.TAG0.ACHIEVEMENT.KILLS> >= 10) && !(<ACCOUNT.TAG0.ACHIEVEMENTS.SET1> & ach_killed10players) )
      ACCOUNT.TAG0.ACHIEVEMENTS.SET1=<EVAL (<ACCOUNT.TAG0.ACHIEVEMENTS.SET1> | ach_killed10players)>   
