   endif
   if ( !<ISIDENTIFIED> )
      SRC.SYSMESSAGE This must be identified before you can understand it
      return 1
   endif
   IF (<eval (<SRC.<eval <more1>>>)> < 1000)
       SRC.SYSMESSAGE You dont feel ready to use this.
       RETURN 1
   ENDIF
   IF (<eval (<SRC.<eval <more1>>>)> >= 1200)
