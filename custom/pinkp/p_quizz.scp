	DIALOGCLOSE d_quizz1
	DIALOGCLOSE d_quizz2
	DIALOGCLOSE d_quizz3
	DIALOGCLOSE d_quizz4
ENDFOR

FORPLAYERS 6144
		TAG.ANSWERED=
		TAG.QUIZZMENU=
ENDFOR


