[FUNCTION quizz] 

IF (<VAR0.QUIZZ>==1)
	IF !(<SRC.TAG0.inevent>==1)
		IF !(<SRC.TAG0.QUIZZMENU>==1)
			IF ( STRCMPI( "<ARGS>", "<VAR0.QUIZZPASSWORD>" ) )
				SRC.SYSMESSAGE You have not entered the correct password!
				RETURN 1
			ELSE
				IF !((STRMATCH("<UID.043971b38.TAG.WINNERACCOUNT3>","<SRC.ACCOUNT>")) || (STRMATCH("<UID.043971b38.TAG.WINNERACCOUNT2>","<SRC.ACCOUNT>")) || (STRMATCH("<UID.043971b38.TAG.WINNERACCOUNT>","<SRC.ACCOUNT>")))
					IF !(<SRC.TAG0.ANSWERED>==1)
						SRC.DIALOG D_QUIZZ1
						SRC.TAG.QUIZZMENU=1
					ELSE
						SRC.SYSMESSAGE You have already answered to this Quizz.
					RETURN 1
					ENDIF
				ELSE
					SRC.SYSMESSAGE You have recently won a Quizz and can't participate in another so fast.
					RETURN 1
				ENDIF
			ENDIF
		ELSE
			SRC.SYSMESSAGE You already openned the Quizz Menu.
			RETURN 1
		ENDIF
	ELSE
		SRC.SYSMESSAGE You cant participate in two different events at the same time!
		RETURN 1
	ENDIF
ELSE
	SRC.DIALOG D_QUIZZ4
ENDIF

