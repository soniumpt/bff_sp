[ITEMDEF i_champion_succubus_spawn]
ID=i_stairs_stone

ON=@CREATE
  COLOR=006a5
  TIMER=1

ON=@TIMER
  IF (0<VAR.SUCCUBUS>>=100)
     LINK.NEWITEM i_transform_smoke
     LINK.ACT.P	= <P>
     LINK.NEWNPC = c_h_championtiffany
     LINK.ACT.P	= <P>
     REGION.ALLCLIENTS SYSMESSAGE @052f The area's champion has been released!
     VAR.SUCCUBUS=0
     TIMER=1
  ELSE
     VAR.SUCCUBUSREMOVE=(0<VAR.SUCCUBUSREMOVE>+1)
     TIMER=1
  ENDIF
  IF (0<VAR.SUCCUBUSREMOVE>=270)
     IF (0<VAR.SUCCUBUS>>0)
        VAR.SUCCUBUSREMOVE=0
        VAR.SUCCUBUS=(0<VAR.SUCCUBUS>-1)
     ENDIF
  ENDIF
  RETURN 1

CATEGORY=Wigifer MPZ
SUBSECTION=Champions Spawns
DESCRIPTION=Succubus Spawn Stone



