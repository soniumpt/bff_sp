   SRC.SAY All Stop
   VAR.BIZZAROGOLD
   SRC.GENERATEBIZZAROGOLD 1
   SRC.UNEQUIPALL
   SRC.UNEQUIPSTEED
   SRC.REVERTBACKPACK
   SRC.BIZZARORESTORESKILLS
   SRC.SAY All Stay
//   IF (<SRC.ACCOUNT.NAME>=="wigstaff")
      // SRC.GO Help Desk of Hell
//      SRC.GO <SRC.TAG.BIZZARO.ORIGIN>
//   ELSE
      SRC.GO <SRC.TAG.BIZZARO.ORIGIN>
//   ENDIF
   SRC.EVENTS -e_bizzaro_shame
   SERV.LOG :: BIZZARO LOG :: <SRC.NAME>:<SRC.ACCOUNT>:<SRC.UID> has left BIZZARO SHAME! They now have <EVAL <SRC.ACCOUNT.TAG0.BIZZARO.SHAME.POINTS>> Bizzaro Shame Points.
   SRC.SYSMESSAGE @51 Well done <SRC.NAME>! You now have <EVAL <SRC.ACCOUNT.TAG0.BIZZARO.SHAME.POINTS>> Bizzaro Shame Points
   RETURN 1
 
// *****************************************************************
//                       Bizzaro Shame Event
// *****************************************************************

[EVENTS e_bizzaro_shame]

ON=@DEATH
   IF !(<NPC>) && ( strmatch("Bizzaro Shame*", "<SRC.REGION.NAME>"))
      SRC.SAY All Stop
      SRC.UNEQUIPALL
      SRC.UNEQUIPSTEED
      SRC.REVERTBACKPACK
      SRC.BIZZARORESTORESKILLS
      SRC.SAY All Stay
      SRC.GO <SRC.TAG.BIZZARO.ORIGIN>
      SRC.EVENTS -e_bizzaro_shame
      SRC.RESURRECT
      SRC.HITS=<SRC.STR>
      SERV.LOG :: BIZZARO LOG :: <SRC.NAME>:<SRC.ACCOUNT>:<SRC.UID> died in BIZZARO SHAME and was warped out! They now have <EVAL <SRC.ACCOUNT.TAG0.BIZZARO.SHAME.POINTS>> Bizzaro Shame Points.
      SRC.SYSMESSAGE @51 Unlucky <SRC.NAME>! You now have <EVAL <SRC.ACCOUNT.TAG0.BIZZARO.SHAME.POINTS>> Bizzaro Shame Points
   ENDIF
   RETURN 1
   
